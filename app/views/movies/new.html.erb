<html>
  <head>
    <title>My first Movie page</title>
  </head>
  <body>
    
    <% form_for @movie do |f| %>
      <%= f.label :director %>
      <%= f.text_field :director %>
      <br/>
      <%= f.label :title %>
      <%= f.text_field :title %>
      <br/>
      <%= f.submit %>
    <% end %>
    
  </body>
</html>